<index version="">
    <module name="">
        <stream name=""> <!-- DNF wants versions and contexts to differ in @summary etc. -->
            <build version="" context="" static="" arch="" summary="" description="">
                <dependencies-or>   <!-- This level not supported by modulemd-packager-v3
                                         Never seen in the wild. Not implemented by DNF. -->
                    <dependencies-and>
                        <dependency name="">
                            <requires></requires> <!-- Only one for modulemd-packager-v3 -->
                            <conflicts></conflicts> <!-- Not supported by modulemd-packager-v3 -->
                        </dependency>
                    </dependencies-and>
                    <dependencies-and>
                        <dependency name=""/> <!-- An unspecified stream.
                                                   Not supported by modulemd-packager-v3. -->
                    </dependencies-and>
                </dependencies-or>
                <servicelevel name="" year="" month="" day=""/>
                <license>
                    <module></module>
                    <content></content>
                </license>
                <references comunity="" documentation="" tracker=""/>
                <profile name="" descripton="">
                    <package></package>
                </profile>
                <api></api>
                <demodularized></demodularized>
                <nevra name="" epoch="" version="" release="" arch="">
                    <checksum type="" value=""/>    <!-- Why does it exist at all? -->
                </nevra>
            </build>
        </stream>
    </module>

    <default modified="" module=""> <!-- @modified could be renamed to version -->
        <intent name="">    <!-- Missing @name means intentless global default -->
            <stream name="" default=""> <!-- @default="1" means settting a default,
                                "0" unsetting, missing or empty no change -->
                <profile></profile> <!-- With a value replaces, empty unsets,
                                         missing no change.  -->
            </stream>
        </intent>
    </default>

    <obsolete modified="" module="" stream="" context="" reset="" eol="" message="">
         <replacement module="" stream=""/>
    </obsolete>

    <translation modified="" module="" stream=""> <!-- version is common with /index -->
        <locale name="" summary="" description="">
            <profile name="" description=""/>
        </locale>
    </translation>
</index>
