<index version="">
    <module name="">
        <stream name="" version="" arch="" summary="" description=""> <!-- The latest version.
                                                    DNF ignores older. Can we move @arch up? -->
            <contexts>
                <context value="" static="">
                    <dependency module="" stream=""/> <!-- only supported by modulemd-packager-v3 -->
                </context>
            </contexts>
            <servicelevel name="" year="" month="" day=""/>
            <license>
                <module></module>
                <content></content>
            </license>
            <references comunity="" documentation="" tracker=""/>
            <profile name="" descripton="">
                <package></package>
            </profile>
            <api></api>
            <demodularized></demodularized>
            <nevra name="" epoch="" version="" release="" arch="">
                <checksum type="" value=""/>    <!-- Why does it exist at all? -->
            </nevra>
        </stream>
    </module>

    <default modified="" module=""> <!-- @modified could be renamed to version -->
        <intent name="">    <!-- Missing @name means intentless global default -->
            <stream name="" default=""> <!-- @default="1" means settting a default,
                                "0" unsetting, missing or empty no change -->
                <profile></profile> <!-- With a value replaces, empty unsets,
                                         missing no change.  -->
            </stream>
        </intent>
    </default>

    <obsolete modified="" module="" stream="" context="" reset="" eol="" message="">
         <replacement module="" stream=""/>
    </obsolete>

    <translation modified="" module="" stream=""> <!-- version is common with /index -->
        <locale name="" summary="" description="">
            <profile name="" description=""/>
        </locale>
    </translation>
</index>
